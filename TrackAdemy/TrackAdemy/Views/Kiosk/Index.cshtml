@{
    ViewBag.Title = "Kiosk Home Page";
}
<style>
 
    /* Create two equal columns that floats next to each other */
    .column {
        float: right;
        width: 33%;
        padding: 20px;
        height: 100px; /* Should be removed. Only for demonstration */
    }

    .row:after {
        content: "";
        padding: 50px;
        display: table;
        clear: both;
    }

    html, body {
        margin: 0;
        padding: 0;
    }

    .Background {
        background: linear-gradient(#B3C65F,#618B42);
        width: 100vw;
        height: 100vh;
        position: absolute;
    }

    .NavBar {
        background-color: #efe8b5;
        width: 100vw;
        height: 100px;
    }

    .center {
        display: block;
        margin-left: auto;
        margin-right: auto;
        width: 50%;
    }

</style>


